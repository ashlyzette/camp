<%- include ("../partials/header") %>
<div class="container">
	<h1 class="text-center">Edit <%=camps.campName%></h1>
	<form action="/camps/<%=camps.id%>?_method=PUT" method="POST">
		<div class="form-group">
			<label for="CampName"></label>
			<input type="text" class ="form-control" name="camp[campName]" id="CampName" value = "<%=camps.campName%>">
			<label for="url"></label>
			<input type="text" class="form-control" name="camp[img]" id="url" value = "<%=camps.img%>">
			<label for="caption"></label>
			<input type="text" class="form-control" name="camp[caption]" id="caption" value = "<%=camps.caption%>">
			<label></label>
			<button type="buton" class = "btn btn-primary btn-block btn-lg">Submit Camp</button>
		</div>
		
	</form>
</div>
<%- include ("../partials/footer") %>